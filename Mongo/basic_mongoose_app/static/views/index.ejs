<body>
	<form action='/users' method='post'>
		Name: <input type='text' name='name'>
		Age: <input type='text' name='age'>
		<input type='submit' name='submit'>
	</form>
	<p>------------------------------------------------------------------------</p>
	<% for(user of users) { %>
		<p><%= user.name %></p>
		<p><%= user.age %></p>
		<p>------------------------</p>
	<% } %>
</body>